    "id": {
      "type": "integer",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "minLength": 2
    },
    "gender": {
      "type": "string"
    },
    "phone": {
      "type": "integer"
    }
  },
  "required": [
    "id",
    "name",
    "gender",
    "phone"
  ]
}